{% extends 'base.html.twig' %}

{% block body %}
   <div class="container">
  <h2>Statistiques par secteur</h2>
	   <p>Nombre de visiteurs par secteur</p>
       <table class="table table-hover">
    <thead>
      <tr>
        <th>Secteur</th>
        <th>Nombre de visiteurs</th>
      </tr>
    </thead>
    <tbody>
        {% for unVisiteurParSecteur in visiteursParSecteur %}
      <tr>
        <td>{{ unVisiteurParSecteur.libelleSec }}</td>
        <td>{{ unVisiteurParSecteur.nbVisiteurs }}</td>
      </tr>
        {% endfor %}
    </tbody>
  </table>
</div>
{% endblock %}
